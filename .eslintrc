{
  "extends": "auth0",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "key-spacing": 0,
    "no-console": 0,
    "no-multi-spaces": 0,
    "react/no-did-mount-set-state": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "react/self-closing-comp": 0,
    "import/no-duplicates": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0
  },
  "globals": {
    "webtaskWidget": true,
    "WebtaskGallery": true,
    "Auth0": true,
    "UglifyJS": true,
    "Raven": true,
    "document": true,
    "window": true,
    "FontFace": true,
    "FormData": true
  }
}
